{
  "name": "portphilio-api",
  "description": "The API backend for Portphilio",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Morgan Benton",
    "email": "registered@morphatic.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "test:all": "npm run eslint && cross-env NODE_ENV= npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "dev": "NODE_ENV=development nodemon src/",
    "dev:ngrok": "NODE_ENV=ngrok nodemon src/",
    "dev:seed": "nodemon src/ --seed",
    "start": "node src/",
    "start:seed": "cross-env NODE_ENV= node src/ --seed",
    "mocha": "mocha test/ --recursive --exit --timeout 10000"
  },
  "dependencies": {
    "@feathersjs/configuration": "^2.0.6",
    "@feathersjs/errors": "^3.3.6",
    "@feathersjs/express": "^1.3.1",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/socketio": "^3.2.9",
    "@morphatic/feathers-auth0": "^0.1.4",
    "@morphatic/feathers-auth0-authorize-hook": "^1.0.3",
    "ajv": "^5.5.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "feathers-hooks-common": "^4.20.7",
    "feathers-memory": "^2.2.0",
    "feathers-mongoose": "^6.3.0",
    "helmet": "^3.18.0",
    "lodash.merge": "^4.6.1",
