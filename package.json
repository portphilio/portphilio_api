{
  "name": "portphilio-api",
  "description": "API backend for Portphilio",
  "version": "0.1.0",
  "homepage": "https://portphil.io",
  "license": "MIT",
  "main": "src",
  "keywords": [
    "portfolios",
    "learning management",
    "portphilio"
  ],
  "author": {
    "name": "Morgan Benton",
    "email": "morgan.benton@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/portphilio/portphilio_api.git"
  },
  "contributors": [],
  "bugs": {
    "url": "git+https://github.com/portphilio/portphilio_api/issues"
  },
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "coverage": "jest --coverage --forceExit",
    "dev": "nodemon src/",
    "dev:ngrok": "cross-env NODE_ENV=ngrok nodemon src/",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "jest": "jest",
    "mongodb": "run-rs -v 4.0.0",
    "start": "node src/",
    "test": "npm run eslint && cross-env NODE_ENV=test npm run jest",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@feathersjs/configuration": "^4.3.4",
    "@feathersjs/errors": "^4.3.4",
    "@feathersjs/express": "^4.3.4",
    "@feathersjs/feathers": "^4.3.4",
    "@feathersjs/socketio": "^4.3.4",
    "@morphatic/feathers-auth0": "^1.0.1",
    "@morphatic/feathers-auth0-strategy": "^1.0.0",
    "aws-sdk": "^2.545.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-blob": "^2.1.0",
    "feathers-hooks-common": "^4.20.7",
    "feathers-memory": "^4.1.0",
    "feathers-mongoose": "^8.1.0",
    "helmet": "^3.21.1",
    "mongodb-core": "^3.2.7",
    "mongoose": "^5.7.3",
    "s3-blob-store": "^3.2.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "ngrok": "^3.2.5",
    "nodemon": "^1.19.3",
    "run-rs": "^0.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
